   <form action="/admin/footer/add"  method="POST"  class="shadow rounded p-4 mb-4">
        <div class="mb-3">
          <label for="exampleInputEmail1" class="form-label">Sayt Nomi</label>
          <input
            type="text"
            name="saytNomi"
            class="form-control"
            id="exampleInputEmail1"
            aria-describedby="emailHelp"
            required
          />
        </div>
        <div class="mb-3">
          <label for="exampleInputEmail2" class="form-label">Manzil</label>
          <input
            type="text"
            name="manzil"
            class="form-control"
            id="exampleInputEmail2"
            aria-describedby="emailHelp"
            required
          />
        </div>
        <div class="mb-3">
          <label for="exampleInputEmail3" class="form-label">Telefon raqam</label>
          <input
            type="text"
            name="telefonRaqam"
            class="form-control"
            id="exampleInputEmail3"
            aria-describedby="emailHelp"
            required
          />
        </div>
        <div class="mb-3">
          <label for="exampleInputEmail3" class="form-label">Email Kiriting</label>
          <input
            type="text"
            name="emailKiriting"
            class="form-control"
            id="exampleInputEmail3"
            aria-describedby="emailHelp"
            required
          />
        </div>
        
        <input type="submit" value="Saqlash" class="btn btn-primary" />
      </form>
      
<div class="container">
  <div class="row">
    <div class="col-lg-12 shadow p-4">
      <table class="table table-hover">
        <thead>
          <tr>
            <th scope="col">â„–</th>
            <th scope="col">Sayt Nomi</th>
            <th scope="col">Manzil</th>
            <th scope="col">Telefon raqam</th>
            <th scope="col">Email Kiriting</th>
            <th scope="col">Tahrirlaah</th>
          </tr>
        </thead>
        <tbody>
          {{#if footer.length}}
            {{#each footer}}
              <tr>
                <td>{{incr @index}}</td>
                <td>{{saytNomi}}</td>
                <td>{{manzil}}</td>
                <td>{{telefonRaqam}}</td>
                <td>{{{emailKiriting}}}</td>
                <td>
                  <a class="btn btn-primary" href="/admin/footer/{{id}}">Tahrirlash</a>
                </td>
              </tr>
            {{/each}}
          {{else}}
            Malumot qo'shilmagan
          {{/if}}
        </tbody>
      </table>
    </div>
  </div>
</div>